{
    "name": "@kurozero/collection",
    "version": "0.3.0",
    "description": "A modern Collection implementation in TypeScript",
    "license": "MIT",
    "main": "lib/index",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "engines": {
        "node": ">=10.x"
    },
    "files": [
        "lib"
    ],
    "author": {
        "name": "Pepijn van den Broek <KurozeroPB>",
        "email": "root@kurozeropb.info",
        "url": "https://kurozeropb.info"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KurozeroPB/Collection.git"
    },
    "homepage": "https://kurozeropb.github.io/Collection/",
    "bugs": {
        "email": "bugs@kurozeropb.info",
        "url": "https://github.com/KurozeroPB/Collection/issues"
    },
    "keywords": [
        "collection",
        "map",
        "typescript"
    ],
    "scripts": {
        "prepare": "yarn run rollup:build && yarn run types:generate",
        "rollup:build": "rollup -c",
        "rollup:watch": "yarn run rollup:build -- --watch",
        "types:generate": "dts-bundle-generator -o lib/index.d.ts src/index.ts",
        "test": "yarn run test:jest && yarn run test:tslint",
        "test:jest": "jest --config jest.config.json --no-cache",
        "test:tslint": "tslint -p ./",
        "docs": "typedoc && cp .nojekyll docs/"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^24.0.13",
        "@typescript-eslint/eslint-plugin": "^1.9.0",
        "@typescript-eslint/parser": "^1.9.0",
        "dts-bundle-generator": "^3.1.0",
        "eslint": "^5.16.0",
        "gts": "^1.0.0",
        "jest": "^24.8.0",
        "rollup": "^1.13.1",
        "rollup-plugin-typescript": "^1.0.1",
        "ts-jest": "^24.0.2",
        "tslib": "^1.9.3",
        "tslint": "^5.17.0",
        "typescript": "^3.5.1"
    }
}
