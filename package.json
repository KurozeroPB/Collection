{
    "name": "@kurozero/collection",
    "version": "0.5.0",
    "description": "A modern Collection implementation in TypeScript",
    "license": "MIT",
    "main": "lib/index",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "engines": {
        "node": ">=10"
    },
    "files": [
        "lib"
    ],
    "author": {
        "name": "Pepijn van den Broek <KurozeroPB>",
        "email": "root@kurozeropb.info",
        "url": "https://kurozeropb.info"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KurozeroPB/Collection.git"
    },
    "homepage": "https://kurozeropb.github.io/Collection/",
    "bugs": {
        "email": "bugs@kurozeropb.info",
        "url": "https://github.com/KurozeroPB/Collection/issues"
    },
    "keywords": [
        "collection",
        "map",
        "typescript"
    ],
    "scripts": {
        "prepare": "yarn build",
        "build": "yarn rollup:build && yarn types:generate && webpack --mode production",
        "rollup:build": "rollup -c",
        "types:generate": "dts-bundle-generator -o lib/index.d.ts src/index.ts",
        "test": "yarn test:jest",
        "test:jest": "jest --config jest.config.js --no-cache",
        "docs": "typedoc && cp .nojekyll docs/",
        "clean": "rm -rf ./node_modules && rm ./yarn.lock"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/jest": "^24.0.22",
        "@types/node": "^12.12.6",
        "@types/webpack": "^4.39.8",
        "@typescript-eslint/eslint-plugin": "^2.6.1",
        "@typescript-eslint/parser": "^2.6.1",
        "dts-bundle-generator": "^3.2.0",
        "eslint": "^6.6.0",
        "gts": "^1.1.0",
        "jest": "^24.9.0",
        "rollup": "^1.26.3",
        "rollup-plugin-typescript": "^1.0.1",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.4.1",
        "tslib": "^1.10.0",
        "typescript": "^3.7.2",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10"
    },
    "resolutions": {
        "**/**/dot-prop": "^5.1.1",
        "**/**/lodash": "^4.17.19",
        "**/**/minimist": "^0.2.1",
        "**/**/acorn": "^7.1.1",
        "**/**/kind-of": "^6.0.3",
        "**/**/serialize-javascript": "^2.1.1",
        "**/**/elliptic": "^6.5.3",
        "**/**/yargs-parser": "^18.1.2",
        "**/**/handlebars": "^4.5.3"
    }
}
