{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "import-name": false,
        "quotemark": [
            true,
            "double",
            "avoid-escape",
            "avoid-template"
        ],
        "ter-indent": [
            true,
            "tab"
        ],
        "trailing-comma": [
            true,
            "never"
        ]
    },
    "rulesDirectory": []
}
